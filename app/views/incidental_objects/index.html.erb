
<p><%= link_to "New incidental object", new_incidental_object_path,:class=>"title" %></p>



<div align="center" id="middle">
  <table border="0" width="100%" cellpadding="0" cellspacing="0" id="product-table">
				<tr>
					<th align="center" valign="middle" class="table-header-repeat line-left minwidth-1"><a href="">Title</a>	</th>
					<th align="center" valign="middle" class="table-header-options line-left"><a href="">Action</a></th>
				</tr>
				

<% @incidental_objects.each do |incidental_object| %>
<tr>
<td>	<%= link_to incidental_object.title,incidental_object %>
   <td>
   <%= link_to '', incidental_object,:class=>"icon-1 info-tooltip" %>
   <%= link_to '', edit_incidental_object_path(incidental_object) ,:class=>"icon-2 info-tooltip"%>
   <a href="/incidental_objects/<%= incidental_object.id %>/delete" onclick="if(confirm('Are you sure you want to delete this page?'))  return true;  else  return false;" class = "icon-3 info-tooltip" ></a>

<% end %>
<tr/>

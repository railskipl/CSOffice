<div id="middle">
<div id="middle_right">
<h1>Sign in</h1>
<%= form_for(:session, :url => sessions_path) do |f| %>
<div class="field">
<%= f.label :email %><br />
<%= f.text_field :email %>
</div>
<div class="field">
<%= f.label :password %><br />
<%= f.password_field :password %>
</div>
<div class="field">
<div class="actions">
	<p><%= link_to "Forgot Password?", new_password_reset_path %></p>
</div>
<%= f.submit "Sign in" %>
</div>
<% end %>
</div>
</div>




<%= form_for(@company_name) do |f| %>
<table> 
	<tr>
		<th align="justify"><%= f.label "Name :" %></th>
		<td><%= f.collection_select :name, ProposedName.find_all_by_client_id(params[:client_id]), :pro_name, :pro_name %></td>
	</tr>
	<%= f.hidden_field :status, :value =>1 %>
    <%= f.hidden_field :client_id, :value => "#{params[:client_id]}" %>
	<tr>
		<td>
		<div class="actions">
		<%= f.submit "Save",:class=>"signup_button"  %>
		</div>
		</td>
		
	</tr>
<% end %>
</table>




 
<center><h1>Listing Company Names</h1><br><center>
	<table cellpadding="10"  align=center border> 
	<tr>
		<th align="center">Company Name
		
	 	<th>Status
	 	<th>Created At
	 	<th colspan="3" align="center">Action
    <% @company_names.each do |company_name| %><br>
	<tr><td align="center"><%= company_name.name %>
       <td><%= link_to format_boolean(company_name.status?), {:controller => 'company_names', :action => 'toggled_status', :id => company_name.id} %>
	<td><%= company_name.created_at.strftime("%B %d, %Y") %>
	  <td><%= link_to 'Show', company_name %>
  	
  	<td><a href="/clients/<%= company_name.client_id %>/requirements/form1a_details/company_names/<%= company_name.id %>/edit">Edit</a>
   	  <td><a href="/company_names/<%= company_name.id %>/delete" onclick="if(confirm('Are you sure you want to delete this page?'))  return true;  else  return false;" >Destroy</a>
    
  
<% end %>
